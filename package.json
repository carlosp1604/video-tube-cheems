{
  "name": "video-tube-cheems",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:up": "docker compose up -d",
    "db:make-migration": "TS_NODE_COMPILER_OPTIONS='{ \"isolatedModules\": false, \"module\": \"commonjs\" }' knex migrate:make -x ts",
    "db:migrate": "TS_NODE_COMPILER_OPTIONS='{ \"isolatedModules\": false, \"module\": \"commonjs\" }' knex migrate:latest",
    "db:rollback": "TS_NODE_COMPILER_OPTIONS='{ \"isolatedModules\": false, \"module\": \"commonjs\" }' knex migrate:rollback",
    "db:make-seed": "TS_NODE_COMPILER_OPTIONS='{ \"isolatedModules\": false, \"module\": \"commonjs\" }' knex seed:make -x ts",
    "db:seed": "TS_NODE_COMPILER_OPTIONS='{ \"isolatedModules\": false, \"module\": \"commonjs\" }' knex seed:run",
    "db:down": "docker compose down",
    "db:destroy": "yarn db:down && sudo rm -rf ./docker/db",
    "db:reset": "yarn db:destroy && yarn db:up && sleep 10 && yarn db:migrate && yarn db:seed",
    "db:import-posts": "ts-node --compiler-options '{ \"isolatedModules\": false, \"module\": \"commonjs\" }' -r tsconfig-paths/register scripts/import-videos.ts",
    "db:import-tags": "ts-node --compiler-options '{ \"isolatedModules\": false, \"module\": \"commonjs\" }' -r tsconfig-paths/register scripts/import-tags.ts",
    "db:import-actors": "ts-node --compiler-options '{ \"isolatedModules\": false, \"module\": \"commonjs\" }' -r tsconfig-paths/register scripts/import-actors.ts",
    "db:import-producers": "ts-node --compiler-options '{ \"isolatedModules\": false, \"module\": \"commonjs\" }' -r tsconfig-paths/register scripts/import-producers.ts",
    "db:import-providers": "ts-node --compiler-options '{ \"isolatedModules\": false, \"module\": \"commonjs\" }' -r tsconfig-paths/register scripts/import-providers.ts",
    "db:update-posts": "ts-node --compiler-options '{ \"isolatedModules\": false, \"module\": \"commonjs\" }' -r tsconfig-paths/register scripts/update-post-urls.ts",
    "db:init": "yarn db:up && sleep 20 && yarn db:migrate && yarn db:import-producers && yarn db:import-actors && yarn db:import-tags && yarn db:import-providers && yarn db:import-posts"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.0",
    "@prisma/client": "^5.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/knex": "^0.16.1",
    "@types/luxon": "^3.1.0",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/react-icons": "^3.0.0",
    "@types/react-transition-group": "^4.4.5",
    "@types/sanitizer": "^0.0.28",
    "@types/uuid": "^9.0.5",
    "awilix": "^8.0.1",
    "bcrypt": "^5.1.0",
    "csstype": "^3.0.10",
    "dotenv": "^16.0.3",
    "exoclick-react": "^1.2.0",
    "isomorphic-dompurify": "^2.6.0",
    "knex": "^2.3.0",
    "luxon": "^3.1.1",
    "mysql2": "^3.9.2",
    "next": "13.4.20-canary.13",
    "next-auth": "^4.6.1",
    "next-sitemap": "^4.2.3",
    "next-translate": "^2.6.2",
    "nextjs-progressbar": "^0.0.16",
    "nodemailer": "^6.9.14",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-ga4": "^2.1.0",
    "react-hot-toast": "^2.4.1",
    "react-hover-video-player": "^10.0.1",
    "react-icons": "^4.11.0",
    "react-tooltip": "5.8.3",
    "react-transition-group": "^4.4.5",
    "sanitizer": "^0.1.3",
    "sass": "^1.57.1",
    "tailwind-scrollbar-hide": "^1.1.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "*",
    "uuid": "^9.0.1",
    "zod": "^3.20.2"
  },
  "main": "index.js",
  "repository": "git@gitlab.com:carlosp1022/video-tube.git",
  "author": "Carlos Ponton <carlitosponton@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@dword-design/eslint-plugin-import-alias": "^3.0.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "next-translate-plugin": "^2.6.2",
    "postcss": "^8.4.8",
    "prisma": "^5.3.1",
    "tailwindcss": "^3.0.23"
  }
}
